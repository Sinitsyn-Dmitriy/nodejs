<div class="container text-center">    
  <h3>To do list</h3>
  <h5>(Use Sesson)</h5>
  <br>
  <div class="row">
    <div class="col-sm-12">


<table class="table table-bordered">
    <thead>
      <tr>
        <th class="form1">Goal</th>
        <th class="form1">Done</th>
    
      </tr>
    </thead>
    <tbody>
      

        <% todolist.forEach(function(todo, index) { %>
  <tr>     
<td  class="col-sm-11"><%= todo %></td>&nbsp;&nbsp;&nbsp;<td><a href="/todo/deleteit/<%= index %>"><button>delete</button></a></td> 

</tr>
        <% }); %>

    </tbody>

  </table>

        <form  method="post" action="/todo/addnew/" id="">
            
                <label for="newtodo" id="form1">Add new task</label>
                <input type="text"  class="" name="newtodo" id="newtodo"  size="75"/>
                <input  class="form1"  type="submit"/>
          
        </form>   


    </div> 
  </div>
</div><br>

